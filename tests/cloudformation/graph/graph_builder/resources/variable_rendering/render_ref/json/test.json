{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "AWS CloudFormation Template to deploy insecure infrastructure",
    "Parameters": {
        "KmsMasterKeyId": {
            "Description": "Company Name",
            "Type": "String"
        },
        "DBName": {
            "Description": "Name of the Database",
            "Type": "String",
            "Default": "db1"
        }
    },
    "Resources": {
        "MySourceQueue": {
            "Type": "AWS::SQS::Queue",
            "Properties": {
                "Tags": [
                  {
                    "Key": "yor_trace",
                    "Value": "c4e9bcea-dcf0-41f9-bb76-e1e0ae444ddf"
                  }
                ],
                "KmsMasterKeyId": {
                    "Ref": "KmsMasterKeyId"
                }
            }
        },
        "MyDB": {
            "Type": "AWS::RDS::DBInstance",
            "Properties": {
                "Tags": [
                  {
                    "Key": "yor_trace",
                    "Value": "56cd2f05-2ad8-4e79-b25f-19fc00210dea"
                  }
                ],
                "DBName": {
                    "Ref": "DBName"
                },
                "DBInstanceClass": "db.t3.micro",
                "Engine": "mysql",
                "MasterUsername": "master",
                "MasterUserPassword": "password"
            }
        }
    },
    "Outputs": {
        "MyDBInstanceName": {
            "Description": "My DB Instance Name",
            "Value": {
                "Ref": "MyDB"
            }
        }
    }
}